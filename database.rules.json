{

  "rules": {
    ".read": "true",
    ".write": "false",
    "users": {
      "$uid": {
        ".read": "true",
        ".write": "auth.uid == $uid",
        ".validate": "newData.hasChildren(['email', 'name', 'picture'])",
        "email": {
          ".read": "false"
        }
      }
    },
    "organizations": {
      ".read": false,
      ".write": "auth != null",
      "$organizationId": {}
    },
    "projects": {
      ".indexOn": ["createdAt"],
      ".write": "auth != null",
      "$projectId": {}
    },
    "countries": {
      ".read": "true",
      ".write": "false"
    },
    "organizationTypes": {
      ".read": "true",
      ".write": "false"
    },
    "skills": {
      ".read": "true",
      ".write": "false"
    }
  }
}
